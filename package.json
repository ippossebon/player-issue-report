{
  "name": "clappr-videos-offer-end-screen",
  "version": "0.1.8",
  "description": "Clappr plugin to display an end screen with a list of videos",
  "main": "./dist/clappr-videos-offer-end-screen.js",
  "scripts": {
    "build": "node_modules/.bin/webpack",
    "start": "node_modules/.bin/webpack-dev-server --host 0.0.0.0 --port 3000 --content-base example/ --hot",
    "artifactory": "npm run test && npm run build && mv ~/npmrc ~/.npmrc && npm publish && mv ~/.npmrc ~/npmrc",
    "test": "npm run build && jasmine",
    "test-watch": "npm run build && jasmine-node spec --autotest --color"
  },
  "repository": {
    "type": "git",
    "url": "gitlab@gitlab.globoi.com:videos-1/clappr-videos-offer-end-screen.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.globoi.com/artifactory/api/npm/npm-local"
  },
  "keywords": [
    "plugin",
    "ui",
    "globosatplay",
    "globoplay",
    "Player",
    "Clappr",
    "end-screen"
  ],
  "author": "videos1@corp.globo.com",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-syntax-function-bind": "^6.5.0",
    "babel-plugin-transform-es5-property-mutators": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "html-loader": "^0.4.3",
    "jasmine": "^2.4.1",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.2.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "moment": "^2.13.0"
  }
}
